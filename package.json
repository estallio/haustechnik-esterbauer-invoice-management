    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "dotenv": "^8.2.0",
